//https://www.codechef.com/problems/STONES

#include <iostream>
using namespace std;

void solve()
{
    string j,l;
    int ans=0;
    cin>>j>>l;
    for(int i=0;i<l.length();i++)
    {
        if(j.find(l[i])!=string::npos)
        {
            ans++;
        }
    }
  cout<<ans<<endl;
}

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    solve();
	}
	return 0;
}
